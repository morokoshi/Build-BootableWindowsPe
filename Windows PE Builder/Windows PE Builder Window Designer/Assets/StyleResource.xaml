                        <Ellipse x:Name="mouseover" Stretch="Fill" Fill="{TemplateBinding Foreground}" HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Stretch" Width="Auto" Visibility="Hidden" Opacity="0.25" Margin="2"/>

